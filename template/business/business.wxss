/* business.wxss */
.business-item{
  background-color: #ffffff;
  position: relative;
  margin:5px;
}
.business-orso{
  margin: 5px;
  align-self: center;
  -moz-align-self:center;
  -webkit-align-self:center;
}
.business-item-img{
  width: 180rpx;
  height: 180rpx;
  background-color: #EBEEF5;
}
.business-item-title{
  margin-bottom: 10px;
  font-size: 15px;
}
.business-item-label text{
  font-size:12px;
  margin-right:20rpx;
  border:1px solid #F49786;
  -webkit-border-radius:3px;
  -moz-border-radius:3px;
  border-radius:3px;
  padding:1px 6px;
  color:#F49786;

}
.business-item-btn{
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 18px;
  background-position: center top;
  height: 25px;
  margin: 0px 5px;
  align-self: center;
  -moz-align-self: center;
  -webkit-align-self: center;
  padding-top:20px;
  font-size:14px;

}
.km{
  font-size:13px; 
  color: #666;
  background-image: url('data:image/png;base64,');
  background-position:left center;
  background-repeat: no-repeat;
  background-size: 15px;
  padding-left: 18px;
}